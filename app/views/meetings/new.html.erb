<h1>New Meeting</h1>
<%= form_for :meeting, url: meetings_path do |f| %>
  <p>
    <%= f.label :date %><br>
    <%= f.text_field :date %>
  </p>
   <p>
    <%= f.label :location %><br>
    <%= f.text_field :location %>
  </p>
 
  <p>
    <%= f.label :participants %><br>
    <%= f.text_area :participants %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>